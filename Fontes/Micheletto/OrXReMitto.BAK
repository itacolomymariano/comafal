      cQuery := " SELECT SED.ED_CODIGO,SE7.E7_ANO,SE7.E7_VALJAN1,SE7.E7_VALFEV1,SE7.E7_VALMAR1,SE7.E7_VALABR1," 
      cQuery += " SE7.E7_VALMAI1,SE7.E7_VALJUN1,SE7.E7_VALJUL1,SE7.E7_VALAGO1,SE7.E7_VALSET1,SE7.E7_VALOUT1,"
