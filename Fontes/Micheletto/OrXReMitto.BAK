      cQuery += " WHERE SED.D_E_L_E_T_ = ' '"
      cQuery += "   AND SUBSTRING(SED.ED_CODIGO,1,6) IN "+FormatIn(cGGNatComl,",")      
      
      MemoWrite("C:\TEMP\ROrcXRea_OrçaGerADM.SQL",cQuery)
      
