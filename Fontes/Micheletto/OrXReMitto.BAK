      TcSetField("ORGAD","E7_VALMAR1","N",14,02)
      TcSetField("ORGAD","E7_VALABR1","N",14,02)
      TcSetField("ORGAD","E7_VALMAI1","N",14,02)
      TcSetField("ORGAD","E7_VALJUN1","N",14,02)
