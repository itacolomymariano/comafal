User Function ChkQtdTp
   lAltQtd := .T.
   cProdNF := GdFieldGet("D1_COD",n)
   cTpPrd  := Posicione("SB1",1,xFilial("SB1")+cProdNF,"B1_TIPO")
   If cTpPrd == "MP"
      lAltQtd := .F.
   EndIf
Return(lAltQtd)