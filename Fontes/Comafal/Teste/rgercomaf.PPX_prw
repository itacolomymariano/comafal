         Cabec(Titulo,Cabec1,Cabec2,NomeProg,Tamanho,nTipo)
         nLin := 8
      Endif
      PrintOut(nLin,000,"|"+Replicate("=",218)+"|",,)

   EndIf

   nLin ++


   lRoda := .F. 
EndDo





Set( 20, "SCREEN" )





If aReturn[5]==1
   dbCommitAll()
