PrintOut(Li,116,If("Q_"$cTipoDif,If(lRastro,Transform(nQuantSB8,cPictQuant),"            "),If(lRastro,"------------","            ")),,)
PrintOut(Li,129,If("Q_"$cTipoDif,If(lLocaliza,Transform(nQuantSBF,cPictQuant),"            "),If(lLocaliza,"------------","            ")),,)
PrintOut(Li,142,If("C_"$cTipoDif,If("B2"$cTipoDif,Transform(nClassSB2,cPictQuant),"            "),If("B2"$cTipoDif,"------------","            ")),,)
PrintOut(Li,155,If("C_"$cTipoDif,If(lRastro,Transform(nClassSB8,cPictQuant),"            "),If(lRastro,"------------","            ")),,)
PrintOut(Li,168,If("C_"$cTipoDif,If(lLocaliza,Transform(nClassSBF,cPictQuant),"            "),If(lLocaliza,"------------","            ")),,)
