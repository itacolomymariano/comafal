			If lLocaliza
				nQuantSBF := 0
				nClassSBF := 0
				nEmpenSBF := 0
				nQuantSB8 := 0
				nClassSB8 := 0
				nEmpenSB8 := 0
				dbSelectArea("SBF")
				If dbSeek(cSeekSBF:=xFilial("SBF")+cCod+cLocal, .F. )
					aLoteSLote := {}
					while !Eof() .And. cSeekSBF==BF_FILIAL+BF_PRODUTO+BF_LOCAL
