	PrintOut(Li,181,If("E_"$cTipoDif,If("B2"$cTipoDif,Transform(nEmpenSB2,cPictQuant),"            "),If("B2"$cTipoDif,"------------","            ")),,)
	PrintOut(Li,194,If("E_"$cTipoDif,If(lRastro,Transform(nEmpenSB8,cPictQuant),"            "),If(lRastro,"------------","            ")),,)
	PrintOut(Li,207,If("E_"$cTipoDif,If(lLocaliza,Transform(nEmpenSBF,cPictQuant),"            "),If(lLocaliza,"------------","            ")),,)
EndIf

Li := Li+1

Return Nil