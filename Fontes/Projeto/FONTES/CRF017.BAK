User Function CRF017(dDataini,dDataFim)

Local aOrd := {}
Local cDesc1        := "Este programa tem como objetivo imprimir relatorio "
Local cDesc2        := "de acordo com os parametros informados pelo usuario."
Local cDesc3        := "Pedido"
Local cPict         := ""
Local titulo        := "Margem de PEDIDOS"
Local nLin          := 80
Local Cabec1        := ""
Local Cabec2        := ""
Local imprime       := .T.
Local cPerg         := "CRF017"

Private CbTxt       := ""
Private lEnd        := .F.
Private lAbortPrint := .F.
Private limite      := 220
Private tamanho     := "G"
Private nomeprog    := "CRF017" // Coloque aqui o nome do programa para impressao no cabecalho
Private nTipo       := 15
Private aReturn     := { "Zebrado", 1, "Administracao", 1, 2, 1, "", 1}
Private nLastKey    := 0
Private cbtxt       := Space(10)
Private cbcont      := 00
Private m_pag       := 01
Private wnrel       := "CRF017" // Coloque aqui o nome do arquivo usado para impressao em disco
Private cString     := "SC5"

DbSelectArea("SC5")
DbSetOrder(2)

DbSelectArea("SC6")
DbSetOrder(1)

AjustaSX1(cPerg)

pergunte(cPerg,.F.)

wnrel := SetPrint(cString,NomeProg,cPerg,@titulo,cDesc1,cDesc2,cDesc3,.F.,aOrd,.F.,Tamanho,,.F.)

If nLastKey == 27
	Return
EndIf

SetDefault(aReturn,cString)

If nLastKey == 27
   Return
EndIf

RptStatus({|| RunReportCRF017(Cabec1,Cabec2,Titulo,nLin) },Titulo)
Return

